{
	"name": "react-express-starterkit",
	"version": "1.0.0",
	"description": "Starter kit for React app with Express/Node backend",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"client-install": "cd client && npm install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/csd713/react-express-starterkit.git"
	},
	"author": "Chiranjeevi SD",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/csd713/react-express-starterkit/issues"
	},
	"homepage": "https://github.com/csd713/react-express-starterkit#readme",
	"dependencies": {
		"concurrently": "^3.5.1",
		"express": "^4.16.3"
	},
	"devDependencies": {
		"nodemon": "^1.17.3"
	}
}